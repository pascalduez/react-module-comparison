{
  "extends": [
    "tslint:latest",
    "tslint-react"
  ],
  "rules": {
    "no-for-in-array": false,
    "quotemark": [
      true,
      "single",
      "jsx-double"
    ],
    "semicolon": [
      true,
      "always",
      "ignore-interfaces"
    ],
    "no-consecutive-blank-lines": [
      true,
      2
    ],
    "object-literal-sort-keys": false,
    "ordered-imports": false,
    "interface-name": false,
    "trailing-comma": [
      true,
      {
        "multiline": {
          "arrays": "always",
          "objects": "always",
          "functions": "never",
          "typeLiterals": "always",
          "imports": "always",
          "exports": "always"
        }
      }
    ]
  }
}
